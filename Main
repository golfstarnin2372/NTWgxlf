        function GetAwaken()
            if S == 1 then
                return "Z"
            elseif S ==2 then
                return "Z, X"
            elseif S ==3  then
                return "Z, X, C"
            elseif S == 4 then
                return "Z X, C, V"
            elseif S == 5 then
                return "Z, X, C, V, F"
